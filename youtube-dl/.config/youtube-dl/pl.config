--audio-format best
--add-metadata
--geo-bypass
--buffer-size 200M
--external-downloader aria2c
--external-downloader-args '-c -j 16 -x 16 -s 16 -k 1M'
-o ~/Videos/$(extractor)s/%(playlist)s/%(playlist_index)s_%(title)s_%(width)sx%(height)s.%(ext)s
--restrict-filenames
-f bestvideo[height<=?1080][fps<=?60]+bestaudio/best
--cookie ~/sync/cookies/youtube
--user-agent 'Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/74.0.3729.169 Safari/537.36'
